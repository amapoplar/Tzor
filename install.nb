     RowBox[{"PrintTemporary", "[", 
      RowBox[{
      "\"\<Copying tzor from \>\"", "<>", "$UserDocumentsDirectory", "<>", 
       "\"\< to\>\"", "<>", "$UserBaseDirectory", "<>", "\"\<...\>\""}], 
      "]"}], ";", "\[IndentingNewLine]", 
